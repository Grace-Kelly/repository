package com.csmf.dto;

import com.csmf.util.SerialNumberType;

public class SerialNumberConfigDto {
    private String defaultValue;

    private SerialNumberType type;

    private Integer length;

    private String seqNoName;

    public SerialNumberConfigDto() {
        super();
    }

    public SerialNumberConfigDto(SerialNumberType type) {
        super();
        this.type = type;
    }

    public SerialNumberConfigDto(SerialNumberType type, String seqNoName, Integer length) {
        super();
        this.type = type;
        this.seqNoName = seqNoName;
        this.length = length;
    }

    public SerialNumberConfigDto(String defaultValue, SerialNumberType type) {
        super();
        this.defaultValue = defaultValue;
        this.type = type;
    }

    public SerialNumberConfigDto(String defaultValue, SerialNumberType type, Integer length) {
        super();
        this.defaultValue = defaultValue;
        this.type = type;
        this.length = length;
    }

    public String getDefaultValue() {
        return defaultValue;
    }

    public void setDefaultValue(String defaultValue) {
        this.defaultValue = defaultValue;
    }

    public SerialNumberType getType() {
        return type;
    }

    public void setType(SerialNumberType type) {
        this.type = type;
    }

    public Integer getLength() {
        return length;
    }

    public void setLength(Integer length) {
        this.length = length;
    }

    public String getSeqNoName() {
        return seqNoName;
    }

    public void setSeqNoName(String seqNoName) {
        this.seqNoName = seqNoName;
    }
}
